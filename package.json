  "scripts": {
    "dev": "vite --mode development",
    "prod": "vite --mode production --port 9159",
    "predeploy": "yarn vite build",
    "deploy": "gh-pages -d dist",
    "build": "tsc -b && vite build --mode production",
    "build:api": "node build-api.js",
    "preview": "vite preview",
